                    </object>
                    <object class="sizeritem">
                        <option>10</option>
                        <flag>wxALL</flag>
                        <border>0</border>
                        <object class="wxChoice" name="approve">
                            <tooltip>true if you wish to vote in favor of that committee_member, false to remove your vote in favor of that committee_member</tooltip>
